{
  "appName": "Savills URL Builder (Demo)",
  "version": "1.0.0",
  "includeYearInPath": true,
  "defaultYear": 2026,
  "markets": [
    {
      "code": "US",
      "label": "United States",
      "baseUrl": "https://www.savills.us"
    },
    {
      "code": "CA",
      "label": "Canada",
      "baseUrl": "https://www.savills.ca"
    }
  ],
  "contentTypes": [
    {
      "code": "research",
      "label": "Research",
      "rootPath": "/insights/research"
    },
    {
      "code": "blog",
      "label": "Blog",
      "rootPath": "/insights/blog"
    }
  ],
  "industries": [
    {
      "code": "retail",
      "label": "Retail"
    },
    {
      "code": "industrial",
      "label": "Industrial"
    },
    {
      "code": "life-sciences",
      "label": "Life Sciences"
    }
  ],
  "slug": {
    "maxLength": 90,
    "stopWords": [
      "the",
      "a",
      "an",
      "and",
      "or",
      "of",
      "to",
      "in",
      "for",
      "with",
      "on",
      "at",
      "by"
    ],
    "normalizeAmpersandTo": "and"
  },
  "registry": {
    "localStorageKey": "savills_url_registry_v1",
    "csvColumns": [
      "created_at",
      "status",
      "title",
      "market",
      "content_type",
      "industry",
      "publish_year",
      "content_id",
      "slug",
      "url_path",
      "full_url",
      "notes"
    ]
  }
}